{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npm run sentry:sourcemaps",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:app": "node dist/app/server/server.mjs",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org kwongtn --project shindig-angular ./dist && sentry-cli sourcemaps upload --org kwongtn --project shindig-angular ./dist",
    "explore": "source-map-explorer dist/app/browser/*.js",
    "analyze": "webpack-bundle-analyzer dist/app/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/fire": "^18.0.1",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/platform-server": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@angular/ssr": "^18.2.5",
    "@ctrl/tinycolor": "^4.1.0",
    "@sentry/angular": "^8.32.0",
    "@sentry/cli": "^2.36.3",
    "express": "^4.18.2",
    "marked": "^12.0.2",
    "monaco-editor": "^0.52.0",
    "ng-zorro-antd": "^18.1.1",
    "ngx-markdown": "^18.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.5",
    "@angular/cli": "^18.2.5",
    "@angular/compiler-cli": "^18.2.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "autoprefixer": "^10.4.21",
    "daisyui": "^5.0.35",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.3",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.5.2",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
